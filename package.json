{
  "name": "fp-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm build:wasm",
    "build:wasm": "wasm-pack build ./wasm --target web --release",
    "watch": "cd wasm && cargo watch -i .gitignore -i \"pkg/*\" -i \"target/*\" -s \"wasm-pack build --target web --dev\"",
    "preview": "vite preview",
    "lint": "prettier --check . && eslint . && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.554.0",
    "@eslint/js": "^9.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/eslint": "^8.56.9",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "autoprefixer": "^10.4.19",
    "crc-32": "^1.2.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.37.0",
    "flowbite": "^2.3.0",
    "flowbite-svelte": "^0.44.24",
    "fp-web-patcher": "^0.4.4",
    "globals": "^15.0.0",
    "postcss": "^8.4.38",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-svelte": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "svelte": "^4.2.14",
    "svelte-check": "^3.6.9",
    "svelte-eslint-parser": "^0.34.1",
    "svelte-preprocess": "^5.1.3",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.6.0",
    "vite": "^5.2.8",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.3.0"
  }
}
